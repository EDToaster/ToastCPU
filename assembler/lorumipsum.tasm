.reset
    imov    r0  .start
    jnz     r0
.vga_addr
    0xC000
.start
    // vga address
    imov    r0 .vga_addr
    load    r0  r0
    imov    r1 .text
    imov    r10 .halt
.loop
    load    r2 r1
    mov     r2 r2
    jz      r10
    // store
    str     r0 r2
    iadd    r0 1
    iadd    r1 1
    imov    r3 .loop
    jnz     r3
.halt
    halt
